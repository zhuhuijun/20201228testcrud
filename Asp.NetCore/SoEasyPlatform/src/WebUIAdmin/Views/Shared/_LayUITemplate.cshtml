
@{
    Layout = "_Layout";
}
@section css{
    <link rel="stylesheet" href="~/layui/css/layui.css" media="all" />
    <link rel="stylesheet" href="~/lib/sweetalert/sweetalert2.min.css" media="all" />
    <link rel="stylesheet" href="~/lib/toastr.js/toastr.min.css" media="all" />
    <link rel="stylesheet" href="~/css/btns.css" media="all" />
    @RenderSection("css", false)
}
@section Scripts{
    <script type="text/javascript" src="~/layui/layui.js"></script>
    <script type="text/javascript" src="~/lib/sweetalert/sweetalert2.min.js"></script>
    <script type="text/javascript" src="~/lib/toastr.js/toastr.min.js"></script>
    <script type="text/javascript" src="~/js/commonutils.js"></script>
    @RenderSection("Scripts", required: false)
}
@RenderBody()